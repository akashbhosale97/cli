{
  "name": "@contentstack/cli-branches",
  "version": "1.0.0",
  "description": "Branch features",
  "main": "lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "prepack": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "branches",
    "branch-merge",
    "dev-dependencies"
  ],
  "author": "contentstack",
  "license": "MIT",
  "dependencies": {
    "@oclif/core": "^1.16.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "eslint": "^8.18.0",
    "fancy-test": "^2.0.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "/lib"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugin": [
      "@contentstack/cli-branches"
    ]
  },
  "repository": "contentstack/cli"
}
